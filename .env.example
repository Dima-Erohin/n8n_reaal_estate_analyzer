# Основные настройки
N8N_HOST=er-real-estate-analyzer.cloudflareaccess.com
N8N_PORT=5678
N8N_PROTOCOL=https
WEBHOOK_URL=https://er-real-estate-analyzer.cloudflareaccess.com/
NODE_ENV=production

# База, которая находится на другом хосте
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=db.example.com
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n_user
DB_POSTGRESDB_PASSWORD=n8n_password
DB_POSTGRESDB_SCHEMA=public

# Где хранить временные файлы n8n
EXECUTIONS_PROCESS=main
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=168

# Локальные пути
N8N_ENCRYPTION_KEY=your_super_secret_key